        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mangel, Auslassung eines leicht zu ergaenzenden Wortes oder Satzteiles, das Gegenteil ist Aposiopese, die gerade das Wichtigste unausgesprochen laesst</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionEllipse1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionEmphase1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionEmphase1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">nachdrueckliche Hervorhebung eines Wortes, oft akustisch oder mit Ausrufezeichen</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionEmphase1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionEnallage -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionEnallage">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vertauschung, Setzung des Adjektivs vor ein anderes Nomen, als zu dem es logisch gehoert, das eigentliche Beziehungswort steht dann oft als Genitivattribut, wird synonym verwendet mit Hypallage</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionEnallage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionEnumeration1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionEnumeration1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Anordnung einer Wort- oder Satzreihe zu einer „koordinierenden Haeufung im Kontakt“ (Lausberg 1990). In der antiken Rhetorik sprachl. Mittel der Beweisfuehrung. Ihre Glieder sind die koordinierten Teile eines Ganzen. Die die Aufzaehlung zusammensetzenden Teile sind grundsaetzlich einander gleichgeordnet (sowohl semantisch als auch syntaktisch). Aufzaehlungen koennen durch die Konjunktionen &quot;und&quot; oder &quot;sowie&quot; verbunden sein. […] Eine steigernde Aufzaehlung heißt Klimax.</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionEnumeration1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionEpanadiplose1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionEpanadiplose1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Wiederholung eines Wortes am Anfang und am Ende eines Satzes/Zeile</grhoot:hatDefinitionsText>
        <rdfs:label>DefinitionEpanadiplose1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionEpanalepse1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionEpanalepse1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Wiederholung eines Wortes oder einer syntakt. Einheit (mit Satzglied- oder Satzfunktion). Die Epanalese verleiht durch die Wiederholung Nachdruck und Eindrücklichkeit</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Helmut Glueck</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Metzler Lexikon der Sprache</grhoot:istQuelle>
        <rdfs:label>DefinitionEpanalepse1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionEpanodos1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionEpanodos1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Wiederholung eines Satzes in umgekehrter Reihenfolge</grhoot:hatDefinitionsText>
        <rdfs:label>DefinitionEpanodos1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionEpipher1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionEpipher1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Umkehr der Anapher, Wiederholung eines oder mehrerer Woerter am Satz- oder Versende</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionEpipher1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionEpitheton1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionEpitheton1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ein schmueckendes Beiwort in formelhafter Verwendung, haeufig bei Homer (der listenreiche Odysseus, der silberne Mond)</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionEpitheton1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionEpithetonOrnans1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionEpithetonOrnans1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ein schmueckendes Beiwort in formelhafter Verwendung, haeufig bei Homer</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionEpithetonOrnans1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionEpizeuxis1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionEpizeuxis1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rhetorische Form der Wiederholung einer drei- oder mehrgliedrigen unmittelbaren Wortfolge</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionEpizeuxis1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionEtymologischeFigur1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionEtymologischeFigur1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Verbindung zweier Woerter desselben Wortstammes, aber verschiedener Wortklassen, z.B. Verb und Nomen</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionEtymologischeFigur1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionEuphemismus1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionEuphemismus1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unangenehmes mit angenehmen Worten sagen, eine beschoenigende Bezeichnung, auch Melioration genannt (Einkaufsparadies fuer Geschaeft).</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionEuphemismus1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionExemplum1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionExemplum1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">beliebig herausgegriffener, typischer Einzelfall als Erklaerung fuer eine bestimmte Erscheinung oder einen bestimmten Vorgang</grhoot:hatDefinitionsText>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Duden</grhoot:istQuelle>
        <rdfs:label>DefinitionExemplum1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionGradation1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionGradation1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">stufenweise Steigerung; Stufenfolge, Oberbegriff fuer Klimax und Antiklimax</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionGradation1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionHendiadyoin1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionHendiadyoin1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Eins durch zwei; Wiedergabe einer Vorstellung durch zwei gleichwertige, mit „und“ verbundene Woerter, wobei die Nebenordnung die logische Unterordnung ersetzt, oder wo zwei mit „und“ verbundene Nomen ein Nomen mit Genitiv- od. Adjektivattribut ersetzen oder wo zwei synonyme Nomina statt eines einzigen Nomens gesetzt werden.</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionHendiadyoin1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionHomoearkton1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionHomoearkton1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Redefigur, bei der die Anfaenge zweier aufeinanderfolgender Woerter gleich oder aehnlich lauten (z. B. Maedchen maehen…)</grhoot:hatDefinitionsText>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Duden</grhoot:istQuelle>
        <rdfs:label>DefinitionHomoearkton1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionHomoeoteleuton1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionHomoeoteleuton1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gleiche Endlaute aufeinanderfolgender Glieder (Reim) (&quot;Nimm lieber Rei - du sparst dabei&quot;)</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Heike Mayer</grhoot:istAutor>
        <rdfs:label>DefinitionHomoeoteleuton1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionHypallage1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionHypallage1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vertauschung eines attributiven Genitivs mit einem attributiven Adjektiv und umgekehrt (z. B. „jagdliche Ausdruecke“ statt „Ausdruecke der Jagd“)</grhoot:hatDefinitionsText>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Duden</grhoot:istQuelle>
        <rdfs:label>DefinitionHypallage1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionHyperbaton1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionHyperbaton1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Das Umgestellte; Zwischenstellung eines Satzgliedes, das eigentlich an einer anderen Stelle im Satz stehen muesste, meist synonym mit Anastrophe und Inversion verwendet, allen drei Stilmitteln ist die empathische Wirkung gemeinsam, die erreicht wird, wenn das zu betonende Wort an den Satz-/Zeilenanfang oder an das SAtz-/Zeilenende gerueckt wird.</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionHyperbaton1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionHyperbel1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionHyperbel1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Uebermaß, ueber ein Ziel hinauswerfen; oft maßlose Ueber- oder Untertreibung</grhoot:hatDefinitionsText>
        <rdfs:label>DefinitionHyperbel1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionHysteronProteron1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionHysteronProteron1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Das Spaetere als Frueheres; Vorwegnahme eines zeitlich oder logisch Nachfolgenden</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionHysteronProteron1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionInterjektion1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionInterjektion1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Woerter oder Wortgruppen, die in einen Satz eingeschoben werden, ohne dass dies syntaktisch gerechtfertigt ist. Sie dienen haeufig dem Ausdruck von Gefuehlen oder der Kontaktaufnahme und sind oft lautmalerisch, z.B. Hoppla!, Brr, Hallo, hallo!</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Helmut Glueck</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Metzler Lexikon der Sprache</grhoot:istQuelle>
        <rdfs:label>DefinitionInterjektion1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionInversion1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionInversion1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Verdrehung; vom normalen grammatischen Satzbau abweichende Umstellung von Satzgliedern (Nach Berlin fahren morgen wir).</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionInversion1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionIronie1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionIronie1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Verstellung, Spott; erheuchelte Unwissenheit, Verstellung; das Gegenteil des Gesagten ist gemeint, Ironie ist kontextabhaendig oder entsteht aus dem Widerspruch zu Person und Situation des sie Verwendenden; sie hat eine Alibifunktion, wenn man dadurch seine wahre Meinung verbirgt, dann ist der Widerspruch zum Kontext nur Eingeweihten erkennbar; sie kann aber auch einen spoettischen und aggressiven Charakter besitzen, wenn man das der Hoerererwartung entsprechende Werturteil in Form eines ironischen Lobes vortraegt.</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionIronie1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionIsokolon1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionIsokolon1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gleiche Worte fast gleicher Silbenzahl stehen in gleichgeordneten Saetzen/Satzteilen, heute wird stattdessen durchweg die Bezeichnung Parallelismus verwendet.</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionIsokolon1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionKatachrese1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionKatachrese1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Missbrauch, Vermengung von nicht zusammengehoerenden Bildern, Stil- oder Bildbruch, Verbindung von mindestens zwei metaphorischen Ausdruecken aus unvereinbaren Bildbereichen
Z.B. &quot;tote Metaphern&quot;: Tischbein; oder &quot;Auch ein blindes Huhn legt mal ein Ei&quot;</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionKatachrese1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionKlimax1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionKlimax1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Leiter, Treppe; eine mindestens dreiteilige Steigerung vom schwaecheren zum staerkeren Ausdruck, gelegentlich auch Gradation genannt, im Deutschen deckt sich diese aber i.e.S. mit der grammatischen Adjektivsteigerung (Komparation), und nur in der Verbindung mit dieser kann sie auch zur Klimax werden</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionKlimax1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionLitotes1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionLitotes1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Schlichtheit, Abschwaechung, Hervorhebung eines Begriffs durch Verneinung des Gegenteils, bei doppelter Verneinung Bejahung, kann ironisch und als Anspielung verwendet werden.</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionLitotes1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionMalapropismus1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionMalapropismus1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">(bewusst) falsche Wortwahl, bei der ein Wort durch ein phonologisch aehnliches, semantisch aber (sehr) unterschiedliches ersetzt wird</grhoot:hatDefinitionsText>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Duden</grhoot:istQuelle>
        <rdfs:label>DefinitionMalapropismus1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionMeiosis1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionMeiosis1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Verringerung; in der Antike verstanden als Gegensatz zur Amplifikation, also zur weitlaeufigen Ausschmueckung, im Lat. Brevitas genannt, in den heutigen Lehrbuechern wird Meiosis nicht mehr als Stilmittel aufgefuehrt, im Deutschen wird stattdessen der Litotes verwendet, im englischen „Understatement“ genannt.</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionMeiosis1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionMelioration1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionMelioration1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Verbesserung, siehe Euphemismus. Beispiel: Liebling statt Ehepartner</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionMelioration1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionMetapher1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionMetapher1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">auf etwas uebetragen; eine Figur bildhaften Sprechens, die Verwendung eines Wortes nicht im eigentlichen (lexikalischen), sondern (uneigentlichen) uebertragenen Sinn; die uebertragung eines Wortes in eine kontextuelle Bildvorstellung unter Beibehaltung eines aehnlichkeitsbezuges. &quot;Eine Metapher ist ein Wort in einem Kontext, durch den es so determiniert wird, dass es etwas anderes meint, als es bedeutet.&quot;</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionMetapher1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionMetathese1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionMetathese1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">(&quot;Umstellung&quot;. Auch: Transposition) Lautwandel, der in der Umstellung eines Lautes oder Vertauschung von Lauten innerhalb von Woertern besteht. Haeufig auch bei Entlehnung und in der Kindersprache. Die Vertauschung kann (a) innerhalb einer Silbe oder (b) von einer Silbe in eine andere erfolgen.</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Helmut Glueck</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Metzler Lexikon der Sprache</grhoot:istQuelle>
        <rdfs:label>DefinitionMetathese1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionMetonomasie1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionMetonomasie1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Umbenennung; Veraenderung des Namens durch uebersetzung oder Angleichung in/an eine andere Sprache, auch Traduktionym genannt</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionMetonomasie1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionMetonymie1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionMetonymie1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Umbenennung, ein gebraeuchliches Wort wird durch ein anderes ersetzt, das zu ihm in enger Beziehung steht (Begriffsubstitution), eng verwandt mit der Synekdoche (pars pro toto)</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionMetonymie1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionNeologismus1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionNeologismus1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Wortneuschoepfung, manchmal gewollt, gewagt und ueberfluessig, manche Neuwoerter werden usualisiert und lexikalisiert oder eingedeutscht</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionNeologismus1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionOnomatopoesie1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionOnomatopoesie1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Laut- oder Klangmalerei. Die sprachliche Nachahmung einer nicht-sprachlichen, meist optischen, akustischen oder haptischen Sinneswahrnehmung, die aber nur imitiert, nicht reproduziert werden kann, sie zielt ab auf die Stimulierung von Emotionen oder sinnlicher oder synaesthetischer Wirkungen bei den Rezipienten</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionOnomatopoesie1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionOxymoron1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionOxymoron1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Verbindung zweier sich ausschließender logischer Begriffe in einem Kompositum oder als contradictio in adiecto (Widerspruch im Beiwort) ein Nomen mit gegensaetzlichem Adjektivattribut</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionOxymoron1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionPalindrom1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionPalindrom1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Eine Buchstabenfolge, die rueckwaerts gelesen denselben Wortlaut oder zumindest einen Sinn ergibt; ist natuerlich kein Stilmittel, kommt aber auch in der Lyrik vor.</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionPalindrom1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionParabel1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionParabel1">
        <rdfs:label>DefinitionParabel1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionParadoxon1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionParadoxon1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">eine scheinbar alogische Behauptung, (die aber hinter dem vordergruendig Unsinnigen auf eine tiefere Wahrheit verweist) eine der ueblichen Meinung widersprechende Formulierung, die dennoch spaeter eine ueberraschende SInnhaftigkeit offenbart, eine der Logik der allgemeinen Meinung und Erfahunrg widersprechende Behauptung, die sich jedoch bei naeherer Betrachtung als richtig erweist, dem allgemein Geglaubten oder Erwarteten zuwiderlaufend.</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <rdfs:label>DefinitionParadoxon1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionParalipse1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionParalipse1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unterlassung; eine rhetorische Figur, bei der man von etwas Bestimmtem erklaert, man wolle es uebergehen, es aber doch erwaehnt und dadurch hervorhebt, oft eingeleitet durch Wendungen wie „ich will nicht davon sprechen, dass …“</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionParalipse1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionParallelismus1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionParallelismus1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Wiederholung derselben syntaktischen Konstruktion in mindestens zwei aufeinander folgenden Satzgliedern (gr. Isokolon) oder in dreien (gr. Trikolon), oft mit einer Steigerung oder wachsenden Gliedern verbunden.</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionParallelismus1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionParaphrase1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionParaphrase1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Alternativformulierung, umschreibende Verdeutlichung. […] Das Wesen der Paraphrase besteht darin, den Inhalt eines Textes mir eigenen Worten wiederzugeben, also das Gleiche, meist auf ein Drittel verkuerzt, mit anderen Worten zu sagen.</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionParaphrase1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionParenthese1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionParenthese1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Einfuegung; Einschub eines satzwertigen (grammatisch vollstaendigen) Satzes in einen anderen Satz, ohne dessen syntaktische Ordnung zu veraendern, durch Gedankenstriche, Klammern oder Kommata gekennzeichnet</grhoot:hatDefinitionsText>
        <rdfs:label>DefinitionParenthese1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionParonomasie1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionParonomasie1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ein Wort umbilden; Wortspiel aus der Klang- und Lautaehnlichkeit zweier stammverschiedener Woerter bei deren Bedeutungsfremdheit, spaeter nur Woerter aehnlichen Klangs, auch Wortspiel aus der aehnlichkeit eines Lautes in zwei Woertern, heute synonym mit Annominatio verwendet, siehe auch: Wortspiel</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionParonomasie1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionPejoration1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionPejoration1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">In der Sprachwissenschaft ein Wort, dessen Bedeutung sich im Lauf der Entwicklung verschlechtert hat</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <rdfs:label>DefinitionPejoration1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionPeriphrase1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionPeriphrase1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Umschreibung durch kennzeichnende Taetigkeiten, Eigenschaften oder Wirkungen, auch zur Vermeidung eines alltaeglichen Ausdruckes oder zur verhuellenden Nennung benutzt, je nach Ausdrucksabsicht erfasst die Periphrase immer nur *ein* Kennzeichen von vielen: Rom - die ewige Stadt, die Hauptstadt Italiens, die Stadt der sieben Huegel.</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Berner</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vollstaendiges Kompendium der rhetorischen Mittel, Stilfiguren und Tropen für Oberstufenschueler und Studienanfaenger</grhoot:istQuelle>
        <rdfs:label>DefinitionPeriphrase1</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionPersonifikation1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionPersonifikation1">
