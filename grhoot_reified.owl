    <!-- https://ramonakuehn.de/grhoot.owl/DefinitionSynonymie1 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/DefinitionSynonymie1">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/RhetorischeDefinition"/>
        <grhoot:hatDefinitionsText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Beziehung der Bedeutungsgleichheit zwischen sprachl. Zeichen. Die ling. Diskussion ueber S. ergab divergierende Einschaetzungen des Begriffs je nachdem, welche Kriterien fuer &quot;Gleichheit&quot; angelegt und welche Existenzformen oder Ebenen von Zeichen ins Auge gefasst wurden.</grhoot:hatDefinitionsText>
        <grhoot:istAutor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Helmut Glueck</grhoot:istAutor>
        <grhoot:istQuelle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Metzler Lexikon der Sprache</grhoot:istQuelle>
