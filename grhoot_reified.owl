    <!-- https://ramonakuehn.de/grhoot.owl/Beispiel4 -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl/Beispiel4">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl/Akrostichon"/>
